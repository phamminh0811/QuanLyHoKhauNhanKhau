      <Button fx:id="tfTachKhau" layoutX="-1.0" layoutY="2.0" mnemonicParsing="false" onAction="#tachKhau" prefHeight="25.0" prefWidth="73.0" text="tách khẩu" />
      <Button layoutX="72.0" layoutY="2.0" mnemonicParsing="false" onAction="#doiChuHo" text="đổi chủ hộ" />
      <TableView fx:id="tvHoKhau" layoutX="-1.0" layoutY="89.0" prefHeight="507.0" prefWidth="800.0" style="-fx-background-color: #CCFFFF;">
        <columns>
          <TableColumn fx:id="colMaHo" prefWidth="148.0" text="mã hộ" />
          <TableColumn fx:id="colMaChuHo" prefWidth="279.0" text="mã chủ hộ" />
            <TableColumn fx:id="colGhiChu" prefWidth="354.0" text="ghi chú" />
        </columns>
         <effect>
            <InnerShadow />
         </effect>
      </TableView>
   </children>
</AnchorPane>
